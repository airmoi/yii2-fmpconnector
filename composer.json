{
    "name": "airmoi/yii2-fmconnector",
    "description": "FileMaker ODBC and PHP-API integration",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","filemaker","ODBC", "FileMaker API-PHP"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Dunand",
            "email": "romain@1-more-thing.com"
        }
    ],
    "minimum-stability": "beta",
    "require": {
        "yiisoft/yii2": "*@stable",
        "airmoi/filemaker": "2.2.6"
    },
    "autoload": {
        "psr-4": {
            "airmoi\\yii2fmconnector\\db\\": "odbc/",
            "airmoi\\yii2fmconnector\\api\\": "api/",
            "airmoi\\yii2fmconnector\\gii\\odbc\\crud\\": "gii/odbc/templates/crud/",
            "airmoi\\yii2fmconnector\\gii\\api\\crud\\": "gii/api/templates/crud/"
        }
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/airmoi/yii2-fmpconnector.git"
        }
    ]
}
