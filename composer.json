{
    "name": "airmoi/yii2-fmconnector",
    "description": "FileMaker ODBC connector and PHP-API integration",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","filemaker","ODBC"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Dunand",
            "email": "airmoi@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "airmoi/filemaker": "*"
    },
    "autoload": {
        "psr-4": {
            "airmoi\\yii2fmconnector\\db\\": "db/",
            "airmoi\\yii2fmconnector\\api\\": "api/",
            "airmoi\\yii2fmconnector\\gii\\crud\\": "gii/templates/crud/"
        }
    },
	"repositories":[
		{
			"type": "git",
			"url": "https://github.com/airmoi/yii2-fmpconnector.git"
		}
	]
}
